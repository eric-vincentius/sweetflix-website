<div class="col-md-4 col-lg-3" id="produk">
    <a href="#" style="text-decoration:none">
        <div class="card h-100">
            <img src="<%= data.imgPath %>" class="card-img-top" alt="<%= data.imgAlt %>" style="height: 150px;">
            <div class="card-body">
                <h5 class="card-title"><%= data.name %></h5>
                <p class="old-price mb-0"><%= data.price %></p>
                <div class="d-flex justify-content-center">
                    <button class="btn btn-info mt-3" id="add-to-cart-btn" data-name="<%= data.name %>"
  data-price="<%= data.price %>" data-img="<%= data.imgPath %>">+ Keranjang</button>
                </div>
            </div>
        </div>
    </a>
</div>